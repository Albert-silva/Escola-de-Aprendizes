CREATE TABLE IF NOT EXISTS `EAprendizes`.`Diretor` (
  `Nome` VARCHAR(255) NOT NULL,
  `CPF` INT(11) NOT NULL,
  `E-mail` VARCHAR(60) NOT NULL,
  `id` INT NOT NULL,
  `Createdat` TIMESTAMP NOT NULL,
  `Edictedat` TIMESTAMP NOT NULL,
  UNIQUE INDEX `CPF_UNIQUE` (`CPF` ASC))
ENGINE = InnoDB
CREATE TABLE IF NOT EXISTS `EAprendizes`.`Professores` (
  `Nome` VARCHAR(255) NOT NULL,
  `CPF` INT(11) NOT NULL,
  `E-mail` VARCHAR(255) NOT NULL,
  `Id` INT NOT NULL,
  `Createdeat` TIMESTAMP NOT NULL,
  `Edictedat` TIMESTAMP NOT NULL,
  UNIQUE INDEX `CPF_UNIQUE` (`CPF` ASC))
ENGINE = InnoDB
CREATE TABLE IF NOT EXISTS `EAprendizes`.`Professores` (
  `Nome` VARCHAR(255) NOT NULL,
  `CPF` INT(11) NOT NULL,
  `E-mail` VARCHAR(255) NOT NULL,
  `Id` INT NOT NULL,
  `Createdeat` TIMESTAMP NOT NULL,
  `Edictedat` TIMESTAMP NOT NULL,
  UNIQUE INDEX `CPF_UNIQUE` (`CPF` ASC))
ENGINE = InnoDB